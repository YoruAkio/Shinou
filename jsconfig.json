{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "baseUrl": "./",
        "paths": {
            "@root/*": ["./*"],
            "@events/*": ["./event/*"],
            "@utils/*": ["./src/utils/*"],
            "@models/*": ["./src/models/*"],
            "@handlers/*": ["./src/handlers/*"]
        }
    },
    "exclude": ["node_modules", "**/node_modules/*"]
}
