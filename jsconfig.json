{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "baseUrl": "./",
        "paths": {
            "@root/*": ["./*"],
            "@events/*": ["Events/*"],
            "@utils/*": ["Utils/*"],
            "@models/*": ["Models/*"],
            "@handlers/*": ["Handlers/*"],
            "@structures/*": ["Structures/*"]
        }
    },
    "exclude": ["node_modules", "**/node_modules/*"]
}
