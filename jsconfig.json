{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "baseUrl": "./",
        "paths": {
            "@root/*": ["./*"],
            "@events/*": ["events/*"],
            "@utils/*": ["utils/*"],
            "@models/*": ["models/*"],
            "@handlers/*": ["handlers/*"],
            "@structures/*": ["structures/*"]
        }
    },
    "exclude": ["node_modules", "**/node_modules/*"]
}
